@model string

@if(!string.IsNullOrEmpty(Model)){
    var statusMessegeClass = Model.StartsWith("Error") ? "Danger" : "Success";
    <div class="alert alert-@statusMessegeClass alert-dismissable" role="alert">
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
        @Model
    </div>
}